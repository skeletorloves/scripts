					Delay(Random(8000,11000))
					}
				}
				else {
					_configname = _shortconfigname;
					if(filepath == "NTMap/char_configs") {
					Print(COLOR_14 + "===========================================================");
					Print(COLOR_14 + "=====>     " + COLOR_3 + "DEFAULT MAP CONFIG LOADED " + COLOR_14 + "     <=====");
					Print(COLOR_14 + "===========================================================");
					}
				}
			}
		break;
	}
	Include(filepath + "/NTConfig.ntl");
	Include(filepath + "/" + _configname);
}

