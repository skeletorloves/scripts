			{
				if(!NTM_MakeTP())
				{
					Say("Failed to make a TP");
				}
				break;
			}
			if(NTC_InTown(me))
			{
				Say("I'm in town! Say 'help tp' for more information.");
			}
			isBusy = 0;
			break;
			
		default:
			var wpID = WaypointNameToID(msg);
			if(wpID == -1)
