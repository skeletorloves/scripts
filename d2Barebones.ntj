// d2Barebones.ntj for script shrinking

function NTMain() {

  d2B_load();   // initializing & loading thingies






}

// common functions

function d2B_load() {
  Include("libs/common/NTCommon.ntl");
	NTC_IncludeLibs();
	NTC_IncludeConfig("NTBot/char_configs");
	NT_LoadConfig();
	NTSI_LoadNIPFiles("NTBot/item_configs");
	NTA_Initialize();
  
  return;
}
